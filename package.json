    }
  },
  "scripts": {
    "dev": "vite",
    "build:docs": "vite build --config vite-docs.config.ts",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/uuid": "^9.0.2",
    "axios": "^1.4.0",
    "compressorjs": "^1.2.1",
    "ez-uploader": "file:./",
    "highlight.js": "^11.8.0",
    "oh-vue-icons": "^1.0.0-rc3",
    "uuid": "^9.0.0",
    "vue": "^3.3.4"
