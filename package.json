{
  "name": "ez-uploader",
  "author": "Mohammad Khayata",
  "private": false,
  "version": "0.6.2",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/Mohammad-Khayata-4441/ez-uploader/tree/v0.4"
  },
  "homepage": "https://ez-uploader.vercel.app/",
  "main": "./dist/ez-uploader.umd.cjs",
  "module": "./dist/ez-uploader.js",
  "types": "./dist/src/lib/main.d.ts",
