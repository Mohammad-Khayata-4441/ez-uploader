{
  "name": "elkood-file-uploader",
  "version": "0.1.0",
  "main": "./dist/FileUploader.umd.js",
  "module": "./dist/FileUploader.es.js",
  "private": false,
  "exports": {
    ".": {
      "import": "./dist/FileUploader.es.js ",
      "require": "./dist/FileUploader.umd.js"
    },
    "./dist/style/file_uploader.css": "./dist/style/file_uploader.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@types/uuid": "^9.0.0",
    "axios": "^1.3.3",
    "compressorjs": "^1.1.1",
    "sass": "^1.58.1",
    "uuid": "^9.0.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
